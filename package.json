{
    "name": "starter-wdio",
    "version": "1.0.0",
    "description": "UI TestAutomation Framework based on WebdriverIO",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "clean": "rimraf .tsbuild && rimraf allure-results && rimraf junit-results",
        "clean-report": "allure generate --clean ./allure-results",
        "wdio": "npm run clean && wdio run test/wdio.conf.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/edanbece31/starter-wdio.git"
    },
    "keywords": [
        "wdio",
        "cucumber",
        "gherkin",
        "typescript"
    ],
    "author": "edanbece",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/edanbece31/starter-wdio/issues"
    },
    "homepage": "https://github.com/edanbece31/starter-wdio#readme",
    "devDependencies": {
        "@badisi/wdio-harness": "^1.0.5",
        "@types/jquery": "^3.5.14",
        "@wdio/allure-reporter": "^7.23.0",
        "@wdio/cli": "^7.23.0",
        "@wdio/cucumber-framework": "^7.23.0",
        "@wdio/junit-reporter": "^7.23.0",
        "@wdio/local-runner": "^7.23.0",
        "@wdio/spec-reporter": "^7.23.0",
        "chromedriver": "^104.0.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.2",
        "wdio-chromedriver-service": "^7.3.2",
        "wdio-wait-for": "^2.2.6"
    },
    "dependencies": {
        "rimraf": "^3.0.2"
    }
}